{
  "name": "demo-package",
  "version": "1.0.0",
  "module": "src/module.js",
  "scripts": {
    "transpile": "babel src -d dist/lib",
    "prepublish-pkg": "npm run transpile && polypackage src --root=dist --out-dir=lib && npm run rollup",
    "rollup": "npm run rollup-es5 && npm run rollup-es2015",
    "rollup-es5": "rollup -c",
    "rollup-es2015": "rollup -c rollup.config-es2015.js",
    "publish-pkg": "npm publish dist"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "polypackage-cli": "*",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1"
  },
  "private": true
}
